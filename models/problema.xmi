<?xml version="1.0" encoding="ASCII"?>
<vtdsl:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:vtdsl="http://www.edu.co/uniandes/miso4202/ehicletax/Vtdsl" nombre="DSL">
  <entidades nombre="Registo" descripcion="Impuesto Vehicular">
    <propiedades xsi:type="vtdsl:Objeto" name="Clave">
      <propieades xsi:type="vtdsl:AtributoSimple" nombre="Placa Vehiculo" tipo="String"/>
    </propiedades>
    <propiedades xsi:type="vtdsl:Objeto" name="Atributos">
      <propieades xsi:type="vtdsl:AtributoListaString" nombre="Marca">
        <valores>Chevrolet</valores>
        <valores>Renault</valores>
        <valores>Mazda</valores>
        <valores>Ford</valores>
        <valores>Audi</valores>
        <valores>BMW</valores>
      </propieades>
      <propieades xsi:type="vtdsl:AtributoListaString" nombre="Uso">
        <valores>Particular</valores>
        <valores>Publico</valores>
      </propieades>
      <propieades xsi:type="vtdsl:AtributoSimple" nombre="Modelo" tipo="Int"/>
      <propieades xsi:type="vtdsl:AtributoSimple" nombre="Linea" tipo="String"/>
      <propieades xsi:type="vtdsl:AtributoListaInteger" nombre="Anio de vigencia">
        <valores>2010</valores>
        <valores>2011</valores>
        <valores>2012</valores>
        <valores>2013</valores>
        <valores>2014</valores>
        <valores>2015</valores>
      </propieades>
    </propiedades>
    <propiedades xsi:type="vtdsl:Objeto" name="OperandosBase" descripcion="rentas y deducciones">
      <propieades xsi:type="vtdsl:AtributoSimple" nombre="Avaluo Comercial" tipo="Real"/>
      <propieades xsi:type="vtdsl:AtributoSimple" nombre="Cilindraje" tipo="Real"/>
      <propieades xsi:type="vtdsl:AtributoListaString" nombre="Tipo Servicio">
        <valores>Particular</valores>
        <valores>Publico</valores>
      </propieades>
      <propieades xsi:type="vtdsl:AtributoSimple" nombre="Modelo" tipo="Int"/>
      <propieades xsi:type="vtdsl:AtributoListaString" nombre="Tipo Vehiculo">
        <valores>Automovil o Camioneta</valores>
        <valores>Motocicleta</valores>
        <valores>Servicio de Carga</valores>
        <valores>Carga de pasajeros</valores>
      </propieades>
    </propiedades>
    <propiedades xsi:type="vtdsl:Objeto" name="OperandosAdicionales">
      <propieades xsi:type="vtdsl:AtributoInicializado" nombre="Semaforizacion" tipo="Real" valor="50000"/>
      <propieades xsi:type="vtdsl:AtributoInicializado" nombre="Descuento pronto pago" tipo="Real" valor="0.1"/>
    </propiedades>
    <propiedades xsi:type="vtdsl:Objeto" name="Formula">
      <propieades xsi:type="vtdsl:Formula" nombre="valorPagar">
        <expresiones xsi:type="vtdsl:Multi">
          <left xsi:type="vtdsl:Multi">
            <left xsi:type="vtdsl:STRING" value="Avaluo Comercial"/>
            <right xsi:type="vtdsl:Div">
              <left xsi:type="vtdsl:Div">
                <left xsi:type="vtdsl:ID" value="Cilindraje"/>
                <right xsi:type="vtdsl:NumberLiteral" value="1000"/>
              </left>
              <right xsi:type="vtdsl:NumberLiteral" value="100"/>
            </right>
          </left>
          <right xsi:type="vtdsl:Minus">
            <left xsi:type="vtdsl:NumberLiteral" value="1"/>
            <right xsi:type="vtdsl:STRING" value="Descuento pronto pago"/>
          </right>
        </expresiones>
      </propieades>
    </propiedades>
  </entidades>
  <entidades nombre="Contacto" descripcion="Contribuyente">
    <propiedades xsi:type="vtdsl:AtributoSimple" nombre="apellidos" tipo="String"/>
    <propiedades xsi:type="vtdsl:AtributoSimple" nombre="nombres" tipo="String"/>
    <propiedades xsi:type="vtdsl:AtributoListaString" nombre="Tipo de identificacion">
      <valores>CC</valores>
      <valores>TI</valores>
      <valores>PT</valores>
      <valores>CE</valores>
    </propiedades>
    <propiedades xsi:type="vtdsl:AtributoSimple" nombre="Numero de identificacion" tipo="String"/>
    <propiedades xsi:type="vtdsl:AtributoListaString" nombre="calidad">
      <valores>propietario</valores>
      <valores>poseedor</valores>
    </propiedades>
    <propiedades xsi:type="vtdsl:AtributoSimple" nombre="Correo Electronico" tipo="String"/>
  </entidades>
  <entidades nombre="Pago" descripcion="Impuesto Anual">
    <propiedades xsi:type="vtdsl:AtributoSimple" nombre="Placa vehiculo" tipo="String"/>
    <propiedades xsi:type="vtdsl:AtributoSimple" nombre="Valor Pagado" tipo="Real"/>
    <propiedades xsi:type="vtdsl:AtributoSimple" nombre="Numero de identificacion" tipo="String"/>
    <propiedades xsi:type="vtdsl:AtributoSimple" nombre="Anio de vigencia" tipo="Int"/>
  </entidades>
</vtdsl:Model>
